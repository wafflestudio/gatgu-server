{
    "AWSEBDockerrunVersion": "1",
    "Logging": "/tmp/gatgu-server",
    "Image": {
        "Name": "noaarhk/gatgu",
        "Update": "true"
    },
    "Ports": [
        {
            "ContainerPort": 8000,
            "HostPort": 8000
        }
    "Volumes": [
        {
          "ContainerDirectory": "/app/assets",
          "HostDirectory": "/var/app/current/assets"
        }
    ]
}
